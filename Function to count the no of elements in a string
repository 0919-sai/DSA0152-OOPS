#include <iostream>
#include <string>

int countElements(const std::string& str) {
    return str.size();
}

int main() {
    std::string input;
    std::cout << "Enter a string: ";
    std::getline(std::cin, input);

    int count = countElements(input);
    std::cout << "Number of elements in the string: " << count << std::endl;

    return 0;
}
