#include <iostream>

int main() {
    int num;

    std::cout << "Enter a positive integer: ";
    std::cin >> num;

    std::cout << "Multiplication table for " << num << ":\n";
    for (int i = 1; i <= 10; i++) {
        std::cout << num << " x " << i << " = " << num * i << std::endl;
    }

    return 0;
}
